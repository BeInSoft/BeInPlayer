package beinplayer.metier;

public enum Genre {
	Rock, Electro, Pop;
	
	public static Genre getGenreByIndex(int id){
		Genre result = Genre.Rock;
		switch (id) {
		case 0:
			result = Genre.Rock;
			break;
		case 1:
			result = Genre.Electro;
			break;
		case 2:
			result = Genre.Electro;
			break;
		}
		return result;
	}
	
}
